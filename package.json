{
  "name": "@stembord/rand",
  "version": "0.1.0",
  "description": "stembord rand utils",
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/stembord/libs/ts-rand.git"
  },
  "main": "./src/index.js",
  "types": "./src/index.d.ts",
  "module": "./lib/index.ts",
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "npm run clean && npm run build",
    "test": "ts-node node_modules/tape/bin/tape ./tests/**/*.test.ts | tap-spec",
    "clean": "npm run clean_build",
    "clean_build": "rm -rf ./src",
    "format.ts": "prettier-tslint fix '**/*.{ts,tsx}'",
    "format.js": "prettier --write '**/*.{js,jsx,json}' --ignore-path .gitignore",
    "format": "npm run format.ts && npm run format.js",
    "lint": "tslint -c tslint.json 'lib/**/*.{ts,tsx}'"
  },
  "keywords": [
    "rand",
    "random"
  ],
  "author": "Nathan Faucett",
  "license": "(MIT OR Apache-2.0)",
  "readmeFilename": "README.md",
  "devDependencies": {
    "@types/tape": "^4.2.33",
    "prettier": "^1.17.0",
    "prettier-tslint": "^0.4.2",
    "tap-spec": "^5.0.0",
    "tape": "^4.10.1",
    "ts-node": "^8.1.0",
    "tslint": "^5.16.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.4.5"
  },
  "dependencies": {
    "@stembord/core": "^0.1.1"
  }
}
